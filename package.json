{
  "name": "veumorphic",
  "version": "0.1.10",
  "description": "Neumorphic UI Kit bundled as Vue Components",
  "files": [
    "dist"
  ],
  "main": "./dist/veumorphic.umd.js",
  "module": "./dist/veumorphic.es.js",
  "exports": {
    ".": {
      "import": "./dist/veumorphic.es.js",
      "require": "./dist/veumorphic.umd.js"
    }
  },
  "repository": "https://github.com/aekazitt/veumorphic",
  "homepage": "https://aekazitt.github.io/veumorphic",
  "author": "Sitt Guruvanich (@aekasitt)",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "build:docs": "cd example && vite build --base /veumorphic --outDir ../docs",
    "dev": "cd example && vite",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "vite": "^2.0.5",
    "vite-plugin-vue2": "^1.4.0",
    "vue-router": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  }
}
